{
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "start:expo": "expo start",
    "start:web": "expo start --web",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web:build": "expo build:web",
    "devtools": "react-devtools",
    "lint": "eslint **/*.{ts,tsx,js,jsx}",
    "lint:fix": "eslint --fix **/*.{ts,tsx,js,jsx}",
    "lint:fix:report": "eslint --fix **/*.{ts,tsx,js,jsx} -o lint-issues.html -f html",
    "format": "prettier \"**/*.{ts,tsx,scss,css,js,json,md}\" --write",
    "test": "jest",
    "test:update": "jest -u",
    "test:coverage": "yarn test --coverage --logHeapUsage --no-cache",
    "test:leak": "yarn test --detectLeaks",
    "postinstall": "jetify",
    "e2e:build": "detox build --configuration ios.sim.debug",
    "e2e:test": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "expo": "^38.0.9",
    "expo-splash-screen": "^0.5.0",
    "expo-status-bar": "^1.0.0",
    "expo-updates": "^0.2.8",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-native": "^0.62.2",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-reanimated": "^1.9.0",
    "react-native-screens": "^2.9.0",
    "react-native-unimodules": "^0.10.0",
    "react-native-web": "^0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/runtime": "^7.10.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.8",
    "@types/react-native": "^0.61.23",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-jsx-control-statements": "^4.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.3",
    "jest-expo": "^38.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.0",
    "release-it": "^13.6.6",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,scss,css,js,json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true
}
